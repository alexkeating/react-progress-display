{
  "name": "react-progress-bar",
  "version": "0.1.0",
  "description": "A progress bar for react.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib",
    "start": "webpack-dev-server --port 3000 --inline --hot",
    "test": "jest --coverage",
    "lint": "eslint --ignore-path .eslintignore --ext .js --ext .jsx ./",
    "fix": "eslint . --fix",
    "prebuild": "npm run clean && npm run lint",
    "build": "babel src -d lib --copy-files",
    "posttest": "npm run lint",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexkeating/react-progress-bar.git"
  },
  "author": "Alexander Keating",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexkeating/react-progress-bar/issues"
  },
  "homepage": "https://github.com/alexkeating/react-progress-bar#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "react-dom": "^15.5.4",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "react": "^15.5.4"
  }
}
